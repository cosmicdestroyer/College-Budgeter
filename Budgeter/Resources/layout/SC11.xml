<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:background="#ff05c9fb"
    android:id="@+id/SC11">
    <TextView
        android:text="Section 8: Home Maintenance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtvSec8TopSC11"
        android:textColor="#ff0002e0"
        android:textSize="23dp"
        android:textStyle="bold|italic"
        android:typeface="serif"
        android:textAlignment="center"
        android:paddingBottom="18dp" />
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ScrollBarSC11" >
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:stretchColumns="1"
            android:id="@+id/TableLaySC11">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Note"
                android:textSize="22dp"
                android:textStyle="bold|italic"
                android:textColor="#ff1a237e"
                android:text="Note: Choose the answer the applies to you the most."
                android:paddingBottom="10dp"
                android:maxLines="5" />
            <TextView
                android:text="1.Do you buy the kitchen maintenance in a bulk for extended periods of time or only for a monthly basis?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Qs1"
                android:textStyle="bold"
                android:textColor="#ffffff00"
                android:textSize="20dp"
                android:maxLines="6"
                android:paddingBottom="5dp" />
            <RadioGroup
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rdgrpSec8Qs1Ans" >
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For an extended period of time"
                    android:id="@+id/rdbtnSec8Qs1Opt1"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp"
                    android:maxLines="2" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a monthly basis"
                    android:id="@+id/rdbtnSec8Qs1Opt2"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a weekly basis"
                    android:id="@+id/rdbtnSec8Qs1Opt3"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
            </RadioGroup>
            <TextView
                android:text="2. Do you buy the toilet maintenance in a bulk for extended periods of time or only for a monthly basis?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Qs1"
                android:textStyle="bold"
                android:textColor="#ffffff00"
                android:textSize="20dp"
                android:maxLines="6"
                android:paddingBottom="5dp" />
            <RadioGroup
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rdgrpSec8Qs2Ans" >
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For an extended period of time"
                    android:id="@+id/rdbtnSec8Qs2Opt1"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp"
                    android:maxLines="2" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a monthly basis"
                    android:id="@+id/rdbtnSec8Qs2Opt2"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a weekly basis"
                    android:id="@+id/rdbtnSec8Qs2Opt3"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
            </RadioGroup>
            <TextView
                android:text="3. Do you buy the room maintenance in a bulk for extended periods of time or only for a monthly basis?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Qs3"
                android:textStyle="bold"
                android:textColor="#ffffff00"
                android:textSize="20dp"
                android:maxLines="6"
                android:paddingBottom="5dp" />
            <RadioGroup
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rdgrpSec8Qs3Ans" >
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For an extended period of time"
                    android:id="@+id/rdbtnSec8Qs3Opt1"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp"
                    android:maxLines="2" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a monthly basis"
                    android:id="@+id/rdbtnSec8Qs3Opt2"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a weekly basis"
                    android:id="@+id/rdbtnSec8Qs3Opt3"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
            </RadioGroup>
            <TextView
                android:text="4. Do you have a backyard in the house you live?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Qs4"
                android:textStyle="bold"
                android:textColor="#ffffff00"
                android:textSize="20dp"
                android:maxLines="4"
                android:paddingBottom="5dp" />
            <RadioGroup
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rdgrpSec8Qs4Ans" >
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Yes"
                    android:id="@+id/rdbtnSec8Qs4Opt1"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="No"
                    android:id="@+id/rdbtnSec8Qs4Opt2"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
            </RadioGroup>
            <TextView
                android:text="4a. Do you buy the backyard maintenance in a bulk for extended periods of time or only for a monthly basis?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtvSec8Q4a"
                android:textStyle="bold"
                android:textColor="#ffff3d00"
                android:textSize="20dp"
                android:maxLines="6"
                android:paddingBottom="5dp" />
            <RadioGroup
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rdgrpSec8Qs2Ans" >
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For an extended period of time"
                    android:id="@+id/rdbtnSec8Qs2Opt1"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp"
                    android:maxLines="2" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a monthly basis"
                    android:id="@+id/rdbtnSec8Qs2Opt2"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="For a weekly basis"
                    android:id="@+id/rdbtnSec8Qs2Opt3"
                    android:buttonTint="#ffd50000"
                    android:textColor="#ffd50000"
                    android:textSize="18dp" />
            </RadioGroup>
            <TextView
                android:text="Click on the 'Proceed' button below to move on to Section 8:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ConclusiveInfoSC11"
                android:textColor="#ff1a237e"
                android:textStyle="bold|italic"
                android:textSize="19dp"
                android:textAlignment="center"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:maxLines="3" />
            <Button
                 android:text="Proceed"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:id="@+id/btnProceedSC11"
                 android:backgroundTint="#ff00e5ff"
                 android:textStyle="bold"
                 android:layout_marginRight="10dp"
                 android:layout_marginLeft="250dp"
                 android:layout_marginTop="10dp"
                 android:textColor="#ff000000" />
             <Button
                 android:text="Back"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:id="@+id/btnBackSC11"
                 android:backgroundTint="#ff00e5ff"
                 android:textStyle="bold"
                 android:layout_marginRight="250dp"
                 android:layout_marginLeft="10dp"
                 android:layout_marginTop="-49dp"
                 android:textColor="#ff000000" />
        </TableLayout>

    </ScrollView>

</LinearLayout>
